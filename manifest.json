{
  "name": "BikeForums.net DeArchiver",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Automatically redirect to non-archive version of page on bikeforums.net",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [ "http://www.bikeforums.net/archive/*" ],
      "js": [ "redirect_to_non_archive.js" ]
    }
  ],
  "permissions": [ "http://www.bikeforums.net/archive/*" ]
}
